# API Configuration
AGENTIC_API_KEY=your_api_key_here

# GitHub Configuration
GITHUB_WEBHOOK_SECRET=your_github_webhook_secret_here
GITHUB_TOKEN=your_github_personal_access_token_here

# GitHub App/Bot Configuration (for status updates and PR/issue creation)
GITHUB_APP_ID=your_github_app_id_here
GITHUB_APP_PRIVATE_KEY_PATH=path_to_github_app_private_key.pem
GITHUB_APP_INSTALLATION_ID=your_installation_id_here

# Email Configuration (for incident notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_specific_password_here
NOTIFICATION_EMAIL_TO=developer@example.com

# Deployment Platforms (optional)
VERCEL_TOKEN=your_vercel_token_here